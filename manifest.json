{
    "manifest_version": 3,
    "name": "Tax Data Fetcher",
    "version": "1.0",
    "description": "Fetch all data by iterating pagination",
    "permissions": [
        "activeTab",
        "scripting",
        "downloads",
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "injected.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}